{
    "name": "blockshiftnetwork/node-red-laravel",
    "description": "Node Red Laravel",
    "keywords": [
        "blockshiftnetwork",
        "node-red-laravel"
    ],
    "homepage": "https://github.com/blockshiftnetwork/node-red-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Blockshift",
            "email": "info@blockshift.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NodeRed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NodeRed\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NodeRed\\NodeRedServiceProvider"
            ],
            "aliases": {
                "NodeRed": "NodeRed\\NodeRedFacade"
            }
        }
    }
}
